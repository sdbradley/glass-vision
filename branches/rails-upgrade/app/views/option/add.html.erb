<h2><%= trn_get('TITLE_OPTION_ADD') %></h2>

<%= form_for :option, @option, :url => {:action => 'create', :id => @option}, :html => {:multipart => true} do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <%= submit_tag trn_geth('BUTTON_ADD') %>
<% end %>
<%= link_to trn_geth('BUTTON_BACK_TO_LIST'), :action => 'list', :mt => @module_type.id %>
